<%= form_for :session, url: login_path do |f| %>
  <div class="form-group">
    <%= f.label :email, 'Enter your email', for: :email %>
    <%= f.text_field :email, class: 'form-control' %>
    <%= f.label :password, 'Enter your password', for: :password %>
    <%= f.text_field :password, type: :password, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Login', class: 'btn btn-primary form-control' %>
    Do not have account? <%= link_to 'Register Now!', new_user_path %><br>
    <%= link_to 'Click here', password_path %> if you don't remember your password.
  </div>
<% end %>